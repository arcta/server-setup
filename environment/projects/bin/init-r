#!/bin/bash

########################################################################
### init project $1 r-server
########################################################################
source ~/.local.cnf

if [ -d "$HOME/projects/$1" ]; then
    cp ~/projects/bin/ui.R ~/projects/$1/ui.R
    cp ~/projects/bin/server.R ~/projects/$1/server.R
else
    echo "Invalid arg: no such project: $1"
fi
